	<queryString>
		<![CDATA[select programa.nombre, programa.proposito_formacion, "programa" AS proceso
FROM programa
WHERE programa.habilitado =1 and $P{proceso}=programa.numero_identificacion and $P{proceso} is not null
union all select programa.nombre, programa.proposito_formacion, "programa" AS proceso
FROM programa
WHERE programa.habilitado=1 and  $P{proceso} is  null 
UNION ALL
SELECT evento.nombre, evento.proposito_formacion, "evento" AS proceso
FROM evento
WHERE evento.habilitado =1 and $P{proceso}=evento.numero_identificacion and $P{proceso} is not null
union all SELECT evento.nombre, evento.proposito_formacion, "evento" AS proceso
FROM evento
WHERE evento.habilitado =1 and  $P{proceso} is  null]]>
	</queryString>
