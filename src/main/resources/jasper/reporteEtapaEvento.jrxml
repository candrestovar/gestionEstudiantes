	<parameter name="evento" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT distinct e.nombre as evento, estudiante.numero_identificacion, concat( estudiante.nombre, " ", estudiante.apellido ) AS nombre, estudiante.email
FROM estudiante
JOIN estudiante_evento ON estudiante_evento.id_estudiante = estudiante.numero_identificacion
 JOIN (
SELECT evento.numero_identificacion AS evento , evento.nombre as nombre
FROM evento
WHERE evento.numero_identificacion =$P{evento} and $P{evento} is not null
union all
SELECT evento.numero_identificacion AS evento, evento.nombre as nombre
FROM evento
where $P{evento} is null
) AS e ON e.evento = estudiante_evento.id_evento]]>
	</queryString>
